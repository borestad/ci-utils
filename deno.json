{
  "fmt": {
    "options": {
      "semiColons": false,
      "singleQuote": true
    }
  },
  "tasks": {
    "dev": "deno run -A --watch job1.ts",
    "job1": "deno run -A job1.ts",
    "update:lockfile": "fd .ts ./bin -tf --threads=1 -X deno cache --import-map=deno.jsonc --unstable --lock=deno.lock --lock-write"
  }
}
